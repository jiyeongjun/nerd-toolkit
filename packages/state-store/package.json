{
  "name": "@nerd-toolkit/state-store",
  "version": "0.1.0",
  "description": "Type-safe state management library with builder pattern",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@fxts/core": "^1.11.0",
    "immer": "^10.1.1"
  },
  "devDependencies": {
    "tsup": "^8.0.2",
    "vitest": "^1.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/nerd-toolkit.git",
    "directory": "packages/state-store"
  },
  "keywords": [
    "state-management",
    "typescript",
    "reactive",
    "store",
    "builder-pattern"
  ],
  "license": "MIT"
}
